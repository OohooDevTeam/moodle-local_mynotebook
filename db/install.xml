<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="local/mynotebook/db" VERSION="20101203" COMMENT="XMLDB file for Moodle local/mynotebook"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="notes" COMMENT="Stores all the notes for every user">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" NEXT="cmid"/>
        <FIELD NAME="cmid" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" UNSIGNED="true" SEQUENCE="false" COMMENT="Course newmodule activity belongs to" PREVIOUS="id" NEXT="courseid"/>
        <FIELD NAME="courseid" TYPE="int" LENGTH="11" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" COMMENT="name field for moodle instances" PREVIOUS="courseid" NEXT="userid"/>
        <FIELD NAME="userid" TYPE="int" LENGTH="11" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" COMMENT="General introduction of the newmodule activity" PREVIOUS="userid" NEXT="name"/>
        <FIELD NAME="name" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" COMMENT="Format of the intro field (MOODLE, HTML, MARKDOWN...)" PREVIOUS="name" NEXT="text"/>
        <FIELD NAME="text" TYPE="text" LENGTH="medium" NOTNULL="false" SEQUENCE="false" PREVIOUS="name" NEXT="location"/>
        <FIELD NAME="location" TYPE="text" LENGTH="medium" NOTNULL="true" SEQUENCE="false" PREVIOUS="text" NEXT="noteurl"/>
        <FIELD NAME="noteurl" TYPE="text" LENGTH="medium" NOTNULL="true" SEQUENCE="false" PREVIOUS="noteurl" NEXT="time_modified"/>
        <FIELD NAME="time_modified" TYPE="text" LENGTH="medium" NOTNULL="true" SEQUENCE="false" PREVIOUS="noteurl" NEXT="deleted"/>
        <FIELD NAME="deleted" TYPE="int" LENGTH="20" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" PREVIOUS="time_,modified"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
  
</XMLDB>